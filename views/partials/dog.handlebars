<div class="col-4 pb-5">
    <div class="gallery-card">
        <div class="gallery-card__side gallery-card__side--front">
            <i id="heart-one-{{this.dataValues.id}}" class="far fa-heart"></i>
            <div class="gallery-card__picture">
                <img src="/{{this.dataValues.photoUrl}}" alt="dog+{{this.dataValues.id}}" />
            </div>
            <h4 class="gallery-card__heading">
                <span
                    class="gallery-card__heading-span gallery-card__heading-span--{{this.dataValues.card_color}}">{{this.dataValues.name}}</span>
            </h4>
            <div class="gallery-card__details">
                <p class="text-small">{{this.dataValues.profile}}</p>
                <div class="gallery-card__details__icons">
                    <div class="row">
                        <div class="col-6">
                            <i class="fas fa-paw"></i>
                            <i class="fas fa-paw"></i>
                        </div>
                        <div class="col-6 gallery-card__details__icons--schedule">
                            <i class="fas fa-calendar-alt"></i> M T W R F S S
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div
            class="gallery-card__side gallery-card__side--back gallery-card__side--back-{{this.dataValues.card_color}}">
            {{#if this.dataValues.available}}
            <div class="gallery-card__cta">
                <i id="heart-two-{{this.dataValues.id}}" class="far fa-heart icon-xl p-5 gallery-card__cta--favorite"
                    data-dogid="{{this.dataValues.id}}"></i>
                <a href="/api/dogs/{{this.dataValues.id}}" class="gallery-card__cta--profile"
                    onClick="myFunction(event)">See Full Profile...</a>
                <a href="/confirmation/{{this.dataValues.id}}"
                    class="btn btn-theme-red btn-lg gallery-card__cta--reserve">Reserve
                    now!</a>
            </div>
            {{else}}
            <div class="gallery-card__cta">
                <i id="heart-two-{{this.dataValues.id}}" class="far fa-heart icon-xl p-5 gallery-card__cta--favorite"
                    data-dogid="{{this.dataValues.name}}"></i>
                <a href="/api/dogs/{{this.dataValues.id}}" class="gallery-card__cta--profile"
                    onClick="myFunction(event)">See Full Profile...</a>
                <a href="#" class="btn btn-theme-red btn-lg gallery-card__cta--reserve">Reserved</a>
            </div>
            {{/if}}

        </div>
    </div>
</div>

<script>
    var k = '{{this.ratings}}'
    console.log(k[0])
    console.log('{{this.ratings}}')
    console.log(JSON.stringify('{{this.ratings}}'))
</script>