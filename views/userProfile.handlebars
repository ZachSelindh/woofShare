<div id="userProfile">
    {{> header }}
    <div class="paw-bg">
    <div class="container mt-5">
        <section class="profile" style="min-height: 400px">
            <div class="row">
                <div class="col-3 profile__left-col">
                    <div class="profile__left-col__img">
                        <img src="{{this.user.dataValues.photo}}" />
                        <h3>{{this.user.dataValues.userName}}</h3>
                    </div>
                    <div class="profile__left-col__menu">
                        <ul class="nav nav-tabs profile__left-col__menu__nav-tabs" id="myTab" role="tablist">
                            <li><a class="nav-item nav-link profile__left-col__menu__nav-link active"
                                    id="nav-active-tab" data-toggle="tab" href="#nav-active" role="tab"
                                    aria-controls="nav-active" aria-selected="true">Active</a></li>
                            <li><a class="nav-item nav-link profile__left-col__menu__nav-link" id="nav-completed-tab"
                                    data-toggle="tab" href="#nav-completed" role="tab" aria-controls="nav-completed"
                                    aria-selected="false">Completed</a></li>
                            <li><a class="nav-item nav-link profile__left-col__menu__nav-link" id="nav-reviewed-tab"
                                    data-toggle="tab" href="#nav-reviewed" role="tab" aria-controls="nav-reviewed"
                                    aria-selected="false">Reviews</a></li>
                            <li><a class="nav-item nav-link profile__left-col__menu__nav-link" id="nav-archived-tab"
                                    data-toggle="tab" href="#nav-archived" role="tab" aria-controls="nav-archived"
                                    aria-selected="false">Archive</a></li>
                        </ul>
                    </div>
                </div>
                <div class="col-9">
                    <div class="tab-content" id="nav-tabContent">
                        {{!-- {{#each rentals }}
                        {{#unless returnComplete}}
                            {{>userActive}}
                        {{/unless}}
                        {{/each}} --}}
                        {{#each rentals }}
                            {{#if returnComplete}}
                                {{#if statusArchive}}
                                    {{>userArchive}}
                                {{else}}
                                    {{>userCompleted}}
                                {{/if}}
                            {{else}}
                                {{>userActive}}
                            {{/if}}
                        {{/each}}

                        <div class="tab-pane fade" id="nav-reviewed" role="tabpanel" aria-labelledby="nav-reviewed-tab">           
                        </div>
                    </div>
                </div>
            </div>
        </section>
    </div>
    </div>
</div>
{{> footer }}